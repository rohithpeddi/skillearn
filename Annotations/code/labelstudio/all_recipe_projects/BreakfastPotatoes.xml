<View>
    <Header value="Video timeline segmentation via AudioPlus sync trick"/>
    <Video name="video" value="$video_url" sync="audio"/>

    <Text name="recipe_name" value="BreakfastPotatoes" granularity="word" highlightColor="#ff0000"/>
    <Text name="text-2" value="Please Select Labels for steps" granularity="word" highlightColor="#ff0000"/>


    <AudioPlus name="audio1" value="$video_url" sync="video" speed="false"/>
    <Labels name="steps" toName="audio1" choice="multiple">
        <Label value="S1: Peel a medium potato." background="#FF0000"/>
        <Label value="S2: Slice the peeled medium potato." background="#FF0000"/>
        <Label value="S3: Peel one medium onion and cut it into two pieces." background="#FF0000"/>
        <Label value="S4: Slice half medium onion into pieces." background="#FF0000"/>
        <Label value="S5: Coat a 9-in. microwave-safe plate with cooking spray" background="#FF0000"/>
        <Label value="S6: Arrange potato and onion slices on the plate" background="#FF0000"/>
        <Label value="S7: Sprinkle potato and onion with seasonings (garlic salt, salt, pepper) on the plate."
               background="#FF0000"/>
        <Label value="S8: Cover the plate and microwave on high for 6 minutes and 30 seconds or until potatoes are tender"
               background="#FF0000"/>
        <Label value="S9: Add 1/4 cup shredded cheddar cheese on the top and microwave for 30 seconds of cooking."
               background="#FF0000"/>
    </Labels>
    <Text name="text-1" value="Please Select Labels for verb and noun" granularity="word" highlightColor="#ff0000"/>
    <AudioPlus name="audio" value="$video_url" sync="video" speed="false"/>

