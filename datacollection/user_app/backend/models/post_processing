from .recording import Recording


class PostProcessing:
	
	def __init__(self, recording: Recording):
		self.recording = recording
		
		self.is_data_synchronized = False
		self.is_data_pushed_to_NAS = False
		self.is_data_pushed_to_BOX_from_NAS = False
		
		self.is_go_pro_360p_video_generated = False
		self.is_go_pro_360p_video_pushed_to_box = False
