    <!-- http://purl.obolibrary.org/obo/FOODON_00002638 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FOODON_00002638">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FOODON_00001211"/>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000122"/>
        <obo:IAO_0000115 xml:lang="en">Penne is an extruded type of pasta with cylinder-shaped pieces, their ends cut at a bias. It is lexically related to Latin penna (meaning &quot;feather&quot; or &quot;quill&quot;), and was originally intended to imitate the then-ubiquitous fountain pen&apos;s steel nibs.</obo:IAO_0000115>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Emma Griffiths</obo:IAO_0000117>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Penne</obo:IAO_0000119>
        <schema:image rdf:resource="https://en.wikipedia.org/wiki/Penne#/media/File:Pennelisce_closeup.png"/>
        <rdfs:label xml:lang="en">penne</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FOODON_00002831 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FOODON_00002831">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FOODON_00001211"/>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000122"/>
        <obo:IAO_0000115 xml:lang="en">Cannelloni are a cylindrical type of lasagna generally served baked with a filling and covered by a sauce in Italian cuisine.</obo:IAO_0000115>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Damion Dooley</obo:IAO_0000117>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Cannelloni</obo:IAO_0000119>
